<table>
  <thead>
  <tr>
    <th>学号</th>
    <th>姓名</th>
    <th>性别</th>
    <th>学院</th>
    <th>年级</th>
    <th>班级</th>
    <th>专业</th>
    <th>电话</th>
    <th>DynEd 班级</th>
    <th>Email</th>
  </tr>
  </thead>
  <tbody>
  <% students.each do |student| %>
    <tr>
      <td><%= student.student_number %></td>
      <td><%= student.name %></td>
      <td><%= student.sex %></td>
      <td><%= student.school %></td>
      <td><%= student.grade %></td>
      <td><%= student.student_class %></td>
      <td><%= student.major %></td>
      <td><%= student.phone_number %></td>
      <td><%= student.dyned_class %></td>
      <td><%= student.email %></td>
    </tr>
  <% end %>
  </tbody>
</table>
<%= paginate students %>